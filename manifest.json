{
    "manifest_version": 3,
    "name": "Smart Bookmark Organizer",
    "version": "1.0",
    "description": "An AI-powered bookmark organizer that categorizes and tags bookmarks for easy retrieval.",
    "action": {
        "default_popup": "popup.html",
        "default_icon": "images/icon.png"
        
    },
    "permissions": [
        "bookmarks",
        "nativeMessaging",
        "storage"
    ],
    "background": {
        "service_worker": "background.js", "type": "module" 
    },
    "icons": {
        "16": "images/icon.png",
        "48": "images/icon.png",
        "128": "images/icon.png"
    }
}
